{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "next dev",
    "build": "next build",
    "start:prod": "rm -rf .next && npm run build && npm run serve",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "lint:all": "npm run lint:js && npm run lint:style && npm run format",
    "lint:all:fix": "npm run lint:js:fix && npm run lint:style:fix && npm run format:fix",
    "lint:js": "next lint",
    "lint:js:fix": "next lint --fix",
    "lint:style": "stylelint **/*.css",
    "lint:style:fix": "stylelint --fix **/*.css",
    "format": "prettier -c .",
    "format:fix": "prettier -w .",
    "codecheck:fix": "npm run test && npm run lint:all:fix"
  },
  "dependencies": {
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.3.0",
    "daisyui": "^4.11.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.1.0",
    "postcss": "^8",
    "prettier": "3.3.0",
    "stylelint": "^16.6.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.4.3",
    "typescript": "^5",
    "vitest": "^1.6.0"
  }
}
